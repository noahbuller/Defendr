{
  "manifest_version": 3,
  "name": "Defendr",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
